{
  "name": "Python on Heroku example",
  "description": "A barebones Python app, which can easily be deployed to Heroku.",
  "image": "heroku/python",
  "repository": "https://github.com/schmico/teamedup",
  "keywords": [
    "python",
    "django"
  ],
  "addons": [
    "heroku-postgresql"
  ]
  "env": [
    "AWS_ACCESS_KEY_ID":[
      "required": true
    ],
    "AWS_SECRET_ACCESS_KEY":[
      "required": true
    ],
  ]
}
